@model IEnumerable<BAI3.Models.LogowanieZdarzen>

@{
    ViewData["Title"] = "Panel";
}

<h1>Panel administratora</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Identyfikator zdarzenia
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Login)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.haslo)
            </th>
            <th>
                Ilość nie udanych logowań
            </th>
            <th>
                Ostatnia próba logowania
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.logowanieZdarzenId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Login)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.haslo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.iloscNieudanychLogowan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ostatniaProbaLogowania)
            </td>
            <td>
                @Html.ActionLink("Edytuj", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Szczegóły", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Usuń", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
}
    </tbody>
</table>
